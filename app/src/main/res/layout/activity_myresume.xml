<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".my.resume.myresumeActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="156dp"
        android:layout_marginBottom="50dp"
        android:src="@mipmap/background_shuimo" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:background="#1E90FF">

        <ImageButton
            android:id="@+id/myresume_back"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="28dp"
            android:background="@mipmap/stucourse_back" />
        <TextView
            android:id="@+id/myresume_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的简历"
            android:textStyle="bold"
            android:textSize="20sp"
            android:textColor="#EFEFEF"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="28dp"/>
        <ImageButton
            android:id="@+id/myresume_add"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:background="@mipmap/myresume_add"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="308dp"
            android:layout_marginLeft="308dp"
            android:layout_marginTop="29dp"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_marginTop="65dp">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="center"
            android:src="@mipmap/stucourse_tabback"
            />
        <TextView
            android:id="@+id/myresume_peo_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="个人简历"
            android:textSize="13sp"
            android:textColor="#000000"
            android:layout_centerVertical="true"
            android:layout_marginLeft="60dp"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:scaleType="fitXY"
            android:src="@mipmap/my_shuxian"
            android:layout_centerInParent="true"/>

        <TextView
            android:id="@+id/myresume_team_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="60dp"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="248dp"
            android:text="团队简历"
            android:textColor="#000000"
            android:textSize="13sp" />
    </RelativeLayout>

   <RelativeLayout
       android:id="@+id/myresume_peolayout"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:layout_marginTop="120dp">

   </RelativeLayout>

</RelativeLayout>