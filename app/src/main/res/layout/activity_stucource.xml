<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".course.stucourse.stucourceActivity"
    android:orientation="vertical"
    android:background="#F0F0F0">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="65dp"
    android:background="#1E90FF">

    <ImageButton
        android:id="@+id/stucourse_back"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="28dp"
        android:background="@mipmap/stucourse_back" />
    <TextView
        android:id="@+id/stucourse_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="服务外包"
        android:textSize="20sp"
        android:textColor="#EFEFEF"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="28dp"/>

</RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="52dp"
        >

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="center"
            android:src="@mipmap/stucourse_tabback"
            />

        <ImageButton
            android:id="@+id/stucourse_recourse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="31dp"
            android:layout_marginLeft="31dp"
            android:layout_marginTop="6dp"
            android:background="@mipmap/stucourse_resource" />
        <TextView
            android:id="@+id/stucourse_recoursetv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="资源"
            android:textSize="12sp"
            android:textColor="#565657"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="29.5dp"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="28dp"/>
        <ImageButton
            android:id="@+id/stucourse_new"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="100dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="6dp"
            android:background="@mipmap/stucourse_new" />
        <TextView
            android:id="@+id/stucourse_newtv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="消息"
            android:textSize="12sp"
            android:textColor="#565657"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="98.5dp"
            android:layout_marginLeft="98.5dp"
            android:layout_marginTop="28dp"/>
        <ImageButton
            android:id="@+id/stucourse_job"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="171dp"
            android:layout_marginLeft="171dp"
            android:layout_marginTop="6dp"
            android:background="@mipmap/stucourse_job" />
        <TextView
            android:id="@+id/stucourse_jobtv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="作业"
            android:textSize="12sp"
            android:textColor="#565657"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="169dp"
            android:layout_marginLeft="169dp"
            android:layout_marginTop="28dp"/>
        <ImageButton
            android:id="@+id/stucourse_live"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="239dp"
            android:layout_marginLeft="239dp"
            android:layout_marginTop="6dp"
            android:background="@mipmap/stucourse_live" />
        <TextView
            android:id="@+id/stucourse_livetv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="直播"
            android:textSize="12sp"
            android:textColor="#565657"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="237.5dp"
            android:layout_marginLeft="237.5dp"
            android:layout_marginTop="28dp"/>
        <ImageButton
            android:id="@+id/stucourse_detail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="312dp"
            android:layout_marginLeft="312dp"
            android:layout_marginTop="6dp"
            android:background="@mipmap/stucourse_detail" />
        <TextView
            android:id="@+id/stucourse_detailtv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="详情"
            android:textSize="12sp"
            android:textColor="#565657"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="308dp"
            android:layout_marginLeft="308dp"
            android:layout_marginTop="28dp"/>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/stucourse_layout"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </LinearLayout>
</LinearLayout>