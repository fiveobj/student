<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".stuloginActivity"
    android:background="@mipmap/signin">
    <ImageButton
        android:id="@+id/stulogin_back"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="30dp"
        android:background="@mipmap/logins_back"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="176dp"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/stulogin_inputbackl"
        android:scaleType="center" />

    <ImageView
        android:layout_width="10dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="193dp"
        android:background="@mipmap/stulogin_phone" />

    <EditText
        android:id="@+id/stulogin_phone"
        android:background="#00FFFFFF"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="191dp"
        android:hint="请输入手机号"
        android:textColor="#D3D3D3"
        android:textSize="20sp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="245dp"
        android:layout_marginLeft="31dp"
        android:background="@mipmap/stulogin_inputbacks"
        android:scaleType="center" />

    <ImageView
        android:layout_width="22dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="264dp"
        android:background="@mipmap/stulogin_code" />

    <EditText
        android:id="@+id/stulogin_code"
        android:background="#00FFFFFF"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="260dp"
        android:hint="验证码"
        android:textColor="#D3D3D3"
        android:textSize="20sp" />

    <ImageButton
        android:id="@+id/stulogin_codebtn"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="245dp"
        android:layout_marginLeft="221dp"
        android:background="#00000000"
        android:src="@mipmap/stulogin_codebtnback" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取验证码"
        android:textSize="15sp"
        android:textColor="#FFFFFF"
        android:layout_marginTop="263dp"
        android:layout_marginLeft="234dp"/>


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="314dp"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/stulogin_inputbackl"
        android:scaleType="center" />

    <ImageView
        android:layout_width="15dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="53dp"
        android:layout_marginTop="332dp"
        android:background="@mipmap/stulogin_password" />

    <EditText
        android:id="@+id/stulogin_pass"
        android:background="#00FFFFFF"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="329dp"
        android:hint="输入密码"
        android:textColor="#D3D3D3"
        android:textSize="18sp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="383dp"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/stulogin_inputbackl"
        android:scaleType="center" />

    <ImageView
        android:layout_width="15dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="53dp"
        android:layout_marginTop="401dp"
        android:background="@mipmap/stulogin_password" />

    <EditText
        android:id="@+id/stulogin_passagain"
        android:background="#00FFFFFF"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="398dp"
        android:hint="再次输入密码"
        android:textColor="#D3D3D3"
        android:textSize="18sp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="452dp"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/stulogin_inputbackl"
        android:scaleType="center" />

    <ImageView
        android:layout_width="24dp"
        android:layout_height="22dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="469dp"
        android:background="@mipmap/stulogin_school" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="521dp"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/stulogin_inputbackl"
        android:scaleType="center" />

    <ImageView
        android:layout_width="24dp"
        android:layout_height="22dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="538dp"
        android:background="@mipmap/stulogin_school" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="590dp"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/stulogin_inputbackl"
        android:scaleType="center" />

    <ImageView
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="607dp"
        android:background="@mipmap/stulogin_stunumber" />

    <EditText
        android:id="@+id/stulogin_stunum"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="605dp"
        android:background="#00FFFFFF"
        android:hint="请输入学号"
        android:textColor="#D3D3D3"
        android:textSize="18sp" />
    <ImageButton
        android:id="@+id/stucourse_backbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/stulogin_btnback"
        android:background="#00000000"
        android:layout_marginTop="659dp"
        android:layout_centerHorizontal="true"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="确认注册"
        android:textColor="#FFFFFF"
        android:layout_marginTop="671dp"
        android:textSize="23sp"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"/>
</RelativeLayout>